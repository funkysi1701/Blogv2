{
  "ci": {
    "assert": {
      "preset": "lighthouse:no-pwa"
    },
    "collect": {
      "settings": {
        "disableStorageReset": true
      },
      "method": "node",
      "additive": false,
      "headful": false,
      "numberOfRuns": 3,
      "url": [
        "https://www.funkysi1701.com/",
        "https://www.funkysi1701.com/metrics/",
        "https://www.funkysi1701.com/pwned-pass",
        "https://www.funkysi1701.com/posts/lone-developer-to-senior-developer-my-2021-story-3g0a",
        "https://www.funkysi1701.com/posts/a-comparison-of-all-good-things-and-star-trek-picard-nb0"
      ]
    },
    "upload": {
      "target": "lhci",
      "serverBaseUrl": "https://lighthousefunkysi.azurewebsites.net/",
      "token": "${{ secrets.LHCI_TOKEN }}"
    }
  }
}
